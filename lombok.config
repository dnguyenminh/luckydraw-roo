# This file is generated by the 'io.freefair.lombok' Gradle plugin
config.stopBubbling = true
lombok.addLombokGeneratedAnnotation = true
# To fix the issue with mapstruct
lombok.accessors.chain = true
lombok.anyConstructor.addConstructorProperties = true
lombok.extern.findbugs.addSuppressFBWarnings = false
lombok.addNullAnnotations = jakarta

# Remove lombok from runtime dependencies
lombok.addLombokGeneratedAnnotation = true

# Generate @NonNull annotations to eliminate null warnings
lombok.nonNull.exceptionType = JDK

# Make @NonNull annotations compatible with Spring requirements
lombok.copyableAnnotations += org.springframework.lang.NonNull
lombok.copyableAnnotations += org.springframework.lang.Nullable
